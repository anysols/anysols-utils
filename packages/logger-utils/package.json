{
  "name": "@p4rm/logger-utils",
  "version": "1.0.4",
  "description": "",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "private": false,
  "scripts": {
    "build": "gulp | tsc",
    "generate-docs": "embedme --stdout --strip-embed-comment README.template.md > README.md",
    "lint": "eslint ./src --ext .ts,.tsx",
    "test": "nyc mocha test/unit/**/*.test.ts"
  },
  "files": [
    "lib",
    "src"
  ],
  "nyc": {
    "check-coverage": false,
    "all": true,
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/**.interface.ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "html",
      "lcov",
      "text",
      "text-summary"
    ],
    "report-dir": "docs/reports/coverage"
  },
  "dependencies": {
    "winston": "^3.3.3"
  },
  "devDependencies": {},
  "keywords": [],
  "author": "",
  "license": "ISC"
